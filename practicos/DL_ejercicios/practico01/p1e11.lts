DIRECTOR = (abrir->DIRECTOR |
			cerrar->DIRECTOR).

EAST = (abrir->EAST |
		cerrar->EAST |
		ingreso->EAST).

WEST = (abrir->WEST |
		cerrar->WEST |
		egreso->WEST).

CONTROLADOR = (abrir->ABIERTO |
				egreso -> CONTROLADOR),

ABIERTO = (ingreso->ABIERTO |
			egreso->ABIERTO |
			cerrar->CONTROLADOR).

||MUSE0 = (DIRECTOR || EAST || WEST || CONTROLADOR).